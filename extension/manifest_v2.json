{
    "manifest_version": 2,
    "name": "NoLink",
    "version": "1.0",
    "description": "Extension that blocks clicking on hyperlinks",
    "icons": {
        "48": "icons/nolink.png",
        "96": "icons/nolink@2x.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["css/style.css"]
        }
    ]
}